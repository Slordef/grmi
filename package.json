{
  "name": "grmi",
  "version": "0.1.0",
  "description": "Github Runners Manager Instance",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/main/index.js",
    "test": "jest --config=jest.config.ts",
    "test:dev": "jest --watch --config=jest.config.ts",
    "cy:dev": "concurrently --kill-others \"pnpm run dev\" \"cypress open\"",
    "dev": "nodemon --files -L --exec ts-node src/main/index.ts",
    "prepare": "husky",
    "lint": "eslint --ext .ts",
    "build": "rimraf ./build && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Slordef/grmi.git"
  },
  "keywords": [
    "grmi",
    "github",
    "action",
    "runner",
    "manager",
    "interface"
  ],
  "author": "Slordef",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Slordef/grmi/issues"
  },
  "homepage": "https://github.com/Slordef/grmi#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.0",
    "@jest/globals": "^30.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "commitlint": "^19.8.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "verify-github-webhook-secret": "^2.0.11",
    "zod": "^3.25.67"
